{
  "kind": "collectionType",
  "collectionName": "website_navbars",
  "info": {
    "singularName": "website-navbar",
    "pluralName": "website-navbars",
    "displayName": "Navbar",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "Items": {
      "type": "dynamiczone",
      "components": [
        "navbar-components.image-link",
        "navbar-components.text-link",
        "navbar-components.nav-button"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "MobileConfig": {
      "type": "component",
      "repeatable": false,
      "component": "navbar-components.mobile-config",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Style": {
      "type": "enumeration",
      "enum": [
        "Spaced",
        "Spread Out",
        "Left Aligned"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": "Spread Out"
    },
    "Appearance": {
      "type": "enumeration",
      "enum": [
        "solid",
        "fade in"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": "solid"
    },
    "FontColor": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    },
    "Name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    }
  }
}
